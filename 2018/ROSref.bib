@article{Gro04,
	title = {Diffusion approximation for a processor sharing queue in heavy traffic},
    year = {2004},
	volume = {14},
	issn = {1050-5164, 2168-8737},
	url = {http://projecteuclid.org/euclid.aoap/1082737104},
	doi = {10.1214/105051604000000035},
	abstract = {Consider a single server queue with renewal arrivals and i.i.d. service times in which the server operates under a processor sharing service discipline. To describe the evolution of this system, we use a measure valued process that keeps track of the residual service times of all jobs in the system at any given time. From this measure valued process, one can recover the traditional performance processes, including queue length and workload. We show that under mild assumptions, including standard heavy traffic assumptions, the (suitably rescaled) measure valued processes corresponding to a sequence of processor sharing queues converge in distribution to a measure valued diffusion process. The limiting process is characterized as the image under an appropriate lifting map, of a one-dimensional reflected Brownian motion. As an immediate consequence, one obtains a diffusion approximation for the queue length process of a processor sharing queue.},
	number = {2},
	urldate = {2016-07-28},
	journal = {Ann. Appl. Probab.},
	author = {Gromoll, H. Christian},
	mrnumber = {MR2052895},
	zmnumber = {1050.60085},
	keywords = {diffusion approximation, Heavy traffic, measure valued process, Processor sharing queue, state space collapse},
	pages = {555--611},
	file = {euclid.aoap.1082737104.pdf:C\:\\Users\\Reza\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\5hlf7mlf.default\\zotero\\storage\\G9B4XTTT\\euclid.aoap.1082737104.pdf:application/pdf;Snapshot:C\:\\Users\\Reza\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\5hlf7mlf.default\\zotero\\storage\\G6WSTDGG\\1082737104.html:text/html}
}

@article{BorEtAl03,
	title = {The equivalence between processor sharing and service in random order},
	volume = {31},
	issn = {0167-6377},
	url = {http://www.sciencedirect.com/science/article/pii/S0167637703000063},
	doi = {10.1016/S0167-6377(03)00006-3},
	abstract = {In this note we explore a useful equivalence relation for the delay distribution in the G/M/1 queue under two different service disciplines: (i) processor sharing (PS); and (ii) random order of service (ROS). We provide a direct probabilistic argument to show that the sojourn time under PS is equal (in distribution) to the waiting time under ROS of a customer arriving to a non-empty system. We thus conclude that the sojourn time distribution for PS is related to the waiting-time distribution for ROS through a simple multiplicative factor, which corresponds to the probability of a non-empty system at an arrival instant. We verify that previously derived expressions for the sojourn time distribution in the M/M/1 PS queue and the waiting-time distribution in the M/M/1 ROS queue are indeed identical, up to a multiplicative constant. The probabilistic nature of the argument enables us to extend the equivalence result to more general models, such as the M/M/1/K queue and ·/M/1 nodes in product-form networks.},
	number = {4},
	urldate = {2016-07-23},
	journal = {Operations Research Letters},
	author = {Borst, S. C. and Boxma, O. J. and Morrison, J. A. and Núñez Queija, R.},
	month = jul,
	year = {2003},
	pages = {254--262},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\Reza\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\5hlf7mlf.default\\zotero\\storage\\32UC9B5V\\Borst et al. - 2003 - The equivalence between processor sharing and serv.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\Reza\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\5hlf7mlf.default\\zotero\\storage\\P5SV45EP\\S0167637703000063.html:text/html}
}

